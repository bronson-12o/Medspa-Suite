# Database Configuration
DATABASE_URL="postgresql://dev:dev@localhost:5432/medspa_local?schema=public"
SHADOW_DATABASE_URL="postgresql://dev:dev@localhost:5432/medspa_local_shadow?schema=public"

# API Configuration
PORT=3001
NODE_ENV=development

# External Service API Keys (PLACEHOLDER VALUES - REPLACE WITH REAL KEYS)
GHL_API_KEY="your_gohighlevel_api_key_here"
GHL_WEBHOOK_SECRET="your_gohighlevel_webhook_secret_here"

# Redis Configuration (if using Bull queue)
REDIS_URL="redis://localhost:6379"

# JWT/Auth Configuration (PLACEHOLDER VALUES)
JWT_SECRET="your_jwt_secret_here_minimum_32_chars_long"
JWT_EXPIRES_IN="7d"

# Webhook URLs (PLACEHOLDER VALUES)
WEBHOOK_BASE_URL="https://your-api-domain.com"

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL="info"